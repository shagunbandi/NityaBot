{
  "agents": {
    "defaults": {
      "model": {
        "primary": "anthropic/claude-opus-4-6"
      },
      "workspace": "/home/node/.openclaw/workspace",
      "heartbeat": {
        "every": "4h",
        "target": "last"
      },
      "subagents": {
        "model": "anthropic/claude-haiku-3-5",
        "maxConcurrent": 4,
        "archiveAfterMinutes": 120
      }
    }
  },
  "gateway": {
    "mode": "local"
  },
  "channels": {
    "whatsapp": {
      "allowFrom": ["+YOUR_NUMBER_HERE"]
    }
  }
}
